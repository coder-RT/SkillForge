generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/**
 * Users
 */

model User {
  userId   String @id @default(uuid())
  userName String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  //@@index([groupName])

  //@@id([userId, groupName])
  @@index([userId])
}
